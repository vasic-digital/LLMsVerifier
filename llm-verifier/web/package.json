{
  "name": "llm-verifier-web",
  "version": "1.0.0",
  "description": "LLM Verifier - Modern Angular Web Interface",
  "keywords": [
    "llm",
    "verification",
    "testing",
    "brotli",
    "compression",
    "monitoring",
    "dashboard"
  ],
  "homepage": "https://github.com/llm-verifier",
  "repository": {
    "type": "git",
    "url": "https://github.com/llm-verifier.git"
  },
  "license": "MIT",
  "author": "LLM Verifier Team",
  "private": false,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 4200 --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "npx playwright test",
    "e2e:headed": "npx playwright test --headed",
    "e2e:debug": "npx playwright test --debug",
    "e2e:report": "npx playwright test --reporter=html",
    "playwright:install": "npx playwright install",
    "playwright:show-report": "npx playwright show-report"
  },
  "dependencies": {
    "@angular/animations": "^17.0.2",
    "@angular/cdk": "^17.0.2",
    "@angular/common": "^17.0.2",
    "@angular/compiler": "^17.0.2",
    "@angular/core": "^17.0.2",
    "@angular/forms": "^17.0.2",
    "@angular/material": "^17.0.2",
    "@angular/platform-browser": "^17.0.2",
    "@angular/platform-browser-dynamic": "^17.0.2",
    "@angular/router": "^17.0.2",
    "@angular/service-worker": "^17.0.2",
    "ngx-lottie": "^10.0.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.2",
    "@angular-devkit/build-webpack": "^0.1700.2",
    "@angular/cli": "^17.0.2",
    "@angular/compiler-cli": "^17.0.2",
    "@playwright/test": "^1.57.0",
    "@types/jasmine": "^5.1.4",
    "@types/node": "^20.10.5",
    "jasmine-core": "~5.1.2",
    "karma": "~6.4.3",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "protractor": "~7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.2.2"
  }
}
