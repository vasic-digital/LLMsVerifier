{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/auth/login": {
            "post": {
                "description": "Authenticate user and return JWT token",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "auth"
                ],
                "summary": "User login",
                "parameters": [
                    {
                        "description": "Login credentials",
                        "name": "credentials",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/api.LoginRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Login response with JWT token",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Validation error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "401": {
                        "description": "Authentication failed",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "Returns the health status of the API",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "system"
                ],
                "summary": "Health check endpoint",
                "responses": {
                    "200": {
                        "description": "Health status",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/models": {
            "get": {
                "description": "Retrieves a list of all LLM models with pagination",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "models"
                ],
                "summary": "Get models",
                "parameters": [
                    {
                        "type": "integer",
                        "description": "Number of results per page (default: 50)",
                        "name": "limit",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "description": "Offset for pagination (default: 0)",
                        "name": "offset",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by provider name",
                        "name": "provider",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "description": "Filter by model status",
                        "name": "status",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "List of models",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Invalid parameters",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            },
            "post": {
                "security": [
                    {
                        "BearerAuth": []
                    }
                ],
                "description": "Creates a new LLM model (admin only)",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "models"
                ],
                "summary": "Create model",
                "parameters": [
                    {
                        "description": "Model data",
                        "name": "model",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/api.CreateModelRequest"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "Created model",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "400": {
                        "description": "Validation error",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "403": {
                        "description": "Forbidden (admin only)",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/system/database-stats": {
            "get": {
                "description": "Returns database performance and size statistics",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "system"
                ],
                "summary": "Get database statistics",
                "responses": {
                    "200": {
                        "description": "Database statistics",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    },
                    "503": {
                        "description": "Database not available",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        },
        "/system/info": {
            "get": {
                "description": "Returns system version, statistics, and counts",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "system"
                ],
                "summary": "Get system information",
                "responses": {
                    "200": {
                        "description": "System information",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "api.CreateModelRequest": {
            "type": "object",
            "required": [
                "model_id",
                "name",
                "provider_id"
            ],
            "properties": {
                "architecture": {
                    "type": "string",
                    "maxLength": 100
                },
                "code_capability_score": {
                    "type": "number",
                    "maximum": 100,
                    "minimum": 0
                },
                "context_window_tokens": {
                    "type": "integer",
                    "minimum": 0
                },
                "deprecated": {
                    "type": "boolean"
                },
                "description": {
                    "type": "string",
                    "maxLength": 500
                },
                "feature_richness_score": {
                    "type": "number",
                    "maximum": 100,
                    "minimum": 0
                },
                "is_multimodal": {
                    "type": "boolean"
                },
                "language_support": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "max_output_tokens": {
                    "type": "integer",
                    "minimum": 0
                },
                "model_id": {
                    "type": "string",
                    "maxLength": 100,
                    "minLength": 1
                },
                "name": {
                    "type": "string",
                    "maxLength": 100,
                    "minLength": 2
                },
                "open_source": {
                    "type": "boolean"
                },
                "overall_score": {
                    "type": "number",
                    "maximum": 100,
                    "minimum": 0
                },
                "parameter_count": {
                    "type": "integer",
                    "minimum": 0
                },
                "provider_id": {
                    "type": "integer",
                    "minimum": 1
                },
                "release_date": {
                    "type": "string"
                },
                "reliability_score": {
                    "type": "number",
                    "maximum": 100,
                    "minimum": 0
                },
                "responsiveness_score": {
                    "type": "number",
                    "maximum": 100,
                    "minimum": 0
                },
                "supports_audio": {
                    "type": "boolean"
                },
                "supports_reasoning": {
                    "type": "boolean"
                },
                "supports_video": {
                    "type": "boolean"
                },
                "supports_vision": {
                    "type": "boolean"
                },
                "tags": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "training_data_cutoff": {
                    "type": "string"
                },
                "use_case": {
                    "type": "string",
                    "maxLength": 200
                },
                "value_proposition_score": {
                    "type": "number",
                    "maximum": 100,
                    "minimum": 0
                },
                "verification_status": {
                    "type": "string",
                    "enum": [
                        "pending",
                        "verified",
                        "failed"
                    ]
                },
                "version": {
                    "type": "string",
                    "maxLength": 50
                }
            }
        },
        "api.LoginRequest": {
            "type": "object",
            "required": [
                "password",
                "username"
            ],
            "properties": {
                "password": {
                    "type": "string",
                    "maxLength": 100,
                    "minLength": 8
                },
                "username": {
                    "type": "string",
                    "maxLength": 50,
                    "minLength": 3
                }
            }
        }
    }
}